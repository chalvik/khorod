{
    "name": "k-gorod/yii2-kg-developer-challenge",
    "description": "Тестовое задание для PHP-Backend-разработчика.",
    "version": "1.0.0",
    "require": {
        "php": ">=7.2",
        "yiisoft/yii2": "~2.0.0"
    },
    "type": "yii2-extension",
    "autoload": {
        "psr-4": {
            "KGorod\\DeveloperChallenge\\": "src"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require-dev": {
        "yiisoft/yii2-gii": "^2.2",
        "codeception/module-phpbrowser": "^1.0",
        "codeception/module-asserts": "^1.3",
        "codeception/codeception": "^4.2",
        "codeception/module-yii2": "^1.1",
        "cebe/markdown": "^1.2",
        "mockery/mockery": "^1.3",
        "codeception/mockery-module": "0.4.0",
        "phpunit/dbunit": "^4.0",
        "phpunit/phpunit": "^7.0"
    },
  "extra": {
    "bootstrap": "KGorod\\DeveloperChallenge\\DeveloperChallengeBootstrap"
  }
}
